{
  "name": "the-shoppies-awards",
  "description": "A webpage that can search OMDB for movies, and allows users to save their favorite films they feel should be up for nomination",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server/app.js",
    "build": "cd client/ && npm install && npm run build",
    "dev": "run-p dev:**",
    "dev:server": "nodemon server/app.js",
    "dev:app": "cd client/ && npm start",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "express": "^4.17.3",
    "http-errors": "^1.6.3",
    "morgan": "^1.9.1",
    "pg": "^8.3.3",
    "react-share": "^4.2.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinellm/the-shoppies-awards.git"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app",
    "react",
    "heroku"
  ],
  "devDependencies": {
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "14.7.0",
    "npm": "6.14.7"
  },
  "author": "Shinell Manwaring (https://shinellmanwaring.com)",
  "bugs": {
    "url": "https://github.com/shinellm/the-shoppies-awards/issues"
  },
